{
  "name": "sanetchek/litevideo",
  "type": "wordpress-plugin",
  "version": "1.0.0",
  "description": "Converts uploaded videos to WebM with VP9 codec for optimized performance in WordPress.",
  "keywords": [
    "WordPress",
    "Plugin",
    "Video",
    "WebM",
    "VP9"
  ],
  "license": "GPL-2.0-or-later",
  "require": {
    "php": ">=7.4"
  },
  "require-dev": {
    "wp-cli/i18n-command": "^2.2.5"
  },
  "scripts": {
    "make-pot": "vendor/bin/wp i18n make-pot . languages/litevideo.pot"
  },
  "autoload": {
    "psr-4": {
      "LiteVideo\\": "src/"
    }
  }
}